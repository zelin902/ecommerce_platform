 </div>\r\n        </div>\r\n        <div class=\"nav-item\" @click=\"switchTab('orders')\">ÊàëÁöÑËÆ¢Âçï</div>\r\n        <div class=\"nav-item\" @click=\"switchTab('user_profile')\">‰∏™‰∫∫‰∏≠ÂøÉ</div>\r\n        <div class=\"nav-item cart\" @click=\"goToCart\">\r\n        Ë¥≠Áâ©ËΩ¶\r\n        <span v-if=\"cartCount > 0\" class=\"cart-badge\">{{ cartCount }}</span>\r\n      </div>\r\n      </nav>\r\n  \r\n      <!-- ÂïÜÂìÅÂàóË°® -->\r\n      <ProductList :products=\"filteredProducts\" @add-to-cart=\"addToCart\" />\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import ProductList from '@/components/ProductList.vue'\r\n  import JSONbig from 'json-bigint'\r\n\r\n  export default {\r\n    name: 'UserHome',\r\n    components: { ProductList },\r\n    data() {\r\n      return {\r\n        searchQuery: '',\r\n        showCategoryDropdown: false,\r\n        cartCount: 0,\r\n        selectedCategory: null,\r\n        username: localStorage.getItem('username') || 'Áî®Êà∑',\r\n        categories: [\r\n          { id: 1, name: 'ÁôΩÈÖí' },\r\n          { id: 2, name: 'Á∫¢ÈÖí' },\r\n          { id: 3, name: 'Âï§ÈÖí' },\r\n          { id: 4, name: 'Ê¥ãÈÖí' },\r\n          { id: 5, name: 'ÈªÑÈÖí' },\r\n          { id: 6, name: 'ÊûúÈÖí' },\r\n          { id: 7, name: '‰øùÂÅ•ÈÖí' },\r\n          { id: 8, name: 'ÂÖ∂‰ªñÈÖíÁ±ª' }\r\n        ],\r\n        products: [], // ÂéüÊù•ÊòØÂÜôÊ≠ªÊï∞ÁªÑÔºåÁé∞Âú®‰∏∫Á©∫\r\n        cartCount: 0 // ÂèØÈÄöËøáÊé•Âè£ÊàñÂÖ®Â±ÄÁä∂ÊÄÅÂêåÊ≠•Ë¥≠Áâ©ËΩ¶Êï∞Èáè\r\n      }\r\n    },\r\n    computed: {\r\n      filteredProducts() {\r\n        let list = this.products\r\n        if (this.selectedCategory) {\r\n          list = list.filter(p => p.category === this.selectedCategory)\r\n        }\r\n        if (this.searchQuery) {\r\n          list = list.filter(p => p.name.includes(this.searchQuery))\r\n        }\r\n        return list\r\n      }\r\n    },\r\n    methods: {\r\n      goToCart() {\r\n      this.$router.push('/cart') // Ë∑≥ËΩ¨Âà∞Ë¥≠Áâ©ËΩ¶Ë∑ØÁî±\r